[ignore]
# Following packages ignored due to type errors.
.*/node_modules/eslint-plugin-jsx-a11y/.*
.*/node_modules/material-ui/test-utils/.*
.*/node_modules/mitt/src/.*
.*/node_modules/babel-plugin-transform-react-remove-prop-types/src/index.js.*
.*/node_modules/jss/lib/plugins/RegularRule.js.flow.*

[include]

[libs]
node_modules/jss/flow-typed
[options]
emoji=true
suppress_comment= \\(.\\|\n\\)*\\(flow-disable-next-line\\|\\$FlowFixMe\\)
unsafe.enable_getters_and_setters=true
